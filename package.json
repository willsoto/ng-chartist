{
  "name": "ng-chartist",
  "version": "0.0.0",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "build": "ng build ng-chartist",
    "checkstyle": "prettier . --check",
    "e2e": "ng e2e",
    "format": "prettier . --write",
    "lint": "ng lint ng-chartist",
    "ng": "ng",
    "prepare": "husky",
    "release": "semantic-release",
    "test": "ng test ng-chartist --watch=false",
    "test:coverage": "ng test ng-chartist --watch=false --code-coverage",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,js,md,json,html}": [
      "prettier --write"
    ]
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ],
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "dist/ng-chartist"
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/git"
    ]
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "20.3.7",
    "@angular-eslint/builder": "20.4.0",
    "@angular-eslint/eslint-plugin": "20.4.0",
    "@angular-eslint/eslint-plugin-template": "20.4.0",
    "@angular-eslint/schematics": "20.4.0",
    "@angular-eslint/template-parser": "20.4.0",
    "@angular/cli": "20.3.7",
    "@angular/common": "20.3.7",
    "@angular/compiler": "20.3.7",
    "@angular/compiler-cli": "20.3.7",
    "@angular/core": "20.3.7",
    "@angular/forms": "20.3.7",
    "@angular/language-service": "20.3.7",
    "@angular/platform-browser": "20.3.7",
    "@angular/platform-browser-dynamic": "20.3.7",
    "@angular/router": "20.3.7",
    "@commitlint/config-conventional": "20.0.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/jasmine": "5.1.12",
    "@types/jasminewd2": "2.0.13",
    "@types/node": "22.18.12",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "chartist": "1.5.0",
    "commitlint": "20.1.0",
    "core-js": "3.46.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsdoc": "61.1.9",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "husky": "9.1.7",
    "jasmine-core": "5.12.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "lint-staged": "16.2.6",
    "ng-packagr": "20.3.0",
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "4.3.0",
    "prettier-plugin-packagejson": "2.5.19",
    "puppeteer": "24.26.1",
    "rxjs": "7.8.2",
    "semantic-release": "25.0.1",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "zone.js": "0.15.1"
  },
  "packageManager": "pnpm@10.19.0+sha512.c9fc7236e92adf5c8af42fd5bf1612df99c2ceb62f27047032f4720b33f8eacdde311865e91c411f2774f618d82f320808ecb51718bfa82c060c4ba7c76a32b8",
  "volta": {
    "node": "22.21.0",
    "pnpm": "10.19.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "core-js",
      "esbuild",
      "lmdb",
      "msgpackr-extract",
      "puppeteer"
    ]
  }
}
