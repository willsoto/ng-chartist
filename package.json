{
  "name": "ng-chartist",
  "version": "0.0.0",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "build": "ng build ng-chartist",
    "checkstyle": "prettier . --check",
    "e2e": "ng e2e",
    "format": "prettier . --write",
    "lint": "ng lint ng-chartist",
    "ng": "ng",
    "prepare": "husky install",
    "release": "semantic-release",
    "test": "ng test ng-chartist --watch=false",
    "test:coverage": "ng test ng-chartist --watch=false --code-coverage",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,js,md,json,html}": [
      "prettier --write"
    ]
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "dist/ng-chartist"
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/git"
    ]
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.2.4",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "15.2.4",
    "@angular/common": "15.2.5",
    "@angular/compiler": "15.2.5",
    "@angular/compiler-cli": "15.2.5",
    "@angular/core": "15.2.5",
    "@angular/forms": "15.2.5",
    "@angular/language-service": "15.2.5",
    "@angular/platform-browser": "15.2.5",
    "@angular/platform-browser-dynamic": "15.2.5",
    "@angular/router": "15.2.5",
    "@commitlint/config-conventional": "17.4.4",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/jasmine": "4.3.1",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "chartist": "1.3.0",
    "commitlint": "17.4.4",
    "core-js": "3.29.1",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "39.9.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "husky": "8.0.3",
    "jasmine-core": "4.6.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "lint-staged": "13.2.0",
    "ng-packagr": "15.2.2",
    "prettier": "2.8.4",
    "prettier-plugin-organize-imports": "3.2.2",
    "prettier-plugin-packagejson": "2.4.3",
    "puppeteer": "19.7.5",
    "rxjs": "7.8.0",
    "semantic-release": "19.0.5",
    "ts-node": "10.9.1",
    "tslib": "2.4.1",
    "typescript": "4.9.5",
    "zone.js": "0.13.0"
  },
  "packageManager": "pnpm@7.19.0",
  "volta": {
    "node": "18.15.0"
  }
}
