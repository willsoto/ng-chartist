{
  "name": "ng-chartist",
  "version": "0.0.0",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "build": "ng build ng-chartist",
    "checkstyle": "prettier . --check",
    "e2e": "ng e2e",
    "format": "prettier . --write",
    "lint": "ng lint ng-chartist",
    "ng": "ng",
    "prepare": "husky install",
    "release": "semantic-release",
    "test": "ng test ng-chartist --watch=false",
    "test:coverage": "ng test ng-chartist --watch=false --code-coverage",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,js,md,json,html}": [
      "prettier --write"
    ]
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "dist/ng-chartist"
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/git"
    ]
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.1.4",
    "@angular-eslint/builder": "16.0.3",
    "@angular-eslint/eslint-plugin": "16.0.3",
    "@angular-eslint/eslint-plugin-template": "16.0.3",
    "@angular-eslint/schematics": "16.0.3",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/cli": "16.1.4",
    "@angular/common": "16.1.3",
    "@angular/compiler": "16.1.3",
    "@angular/compiler-cli": "16.1.3",
    "@angular/core": "16.1.3",
    "@angular/forms": "16.1.3",
    "@angular/language-service": "16.1.3",
    "@angular/platform-browser": "16.1.3",
    "@angular/platform-browser-dynamic": "16.1.3",
    "@angular/router": "16.1.3",
    "@commitlint/config-conventional": "17.6.6",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/jasmine": "4.3.5",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "18.16.19",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "chartist": "1.3.0",
    "commitlint": "17.6.6",
    "core-js": "3.31.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "46.4.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "husky": "8.0.3",
    "jasmine-core": "5.0.1",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "lint-staged": "13.2.3",
    "ng-packagr": "16.1.0",
    "prettier": "3.0.0",
    "prettier-plugin-organize-imports": "3.2.2",
    "prettier-plugin-packagejson": "2.4.4",
    "puppeteer": "20.8.0",
    "rxjs": "7.8.1",
    "semantic-release": "21.0.7",
    "ts-node": "10.9.1",
    "tslib": "2.6.0",
    "typescript": "5.1.6",
    "zone.js": "0.13.1"
  },
  "packageManager": "pnpm@8.6.6",
  "volta": {
    "node": "18.16.1",
    "pnpm": "8.6.0"
  }
}
