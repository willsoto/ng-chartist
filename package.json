{
  "name": "ng-chartist",
  "version": "0.0.0",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "build": "ng build ng-chartist",
    "checkstyle": "prettier . --check",
    "e2e": "ng e2e",
    "format": "prettier . --write",
    "lint": "ng lint ng-chartist",
    "ng": "ng",
    "prepare": "husky install",
    "release": "semantic-release",
    "test": "ng test ng-chartist --watch=false",
    "test:coverage": "ng test ng-chartist --watch=false --code-coverage",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,js,md,json,html}": [
      "prettier --write"
    ]
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ],
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "dist/ng-chartist"
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/git"
    ]
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.0.1",
    "@angular-eslint/builder": "18.0.0-alpha.16",
    "@angular-eslint/eslint-plugin": "18.0.0-alpha.16",
    "@angular-eslint/eslint-plugin-template": "18.0.0-alpha.16",
    "@angular-eslint/schematics": "18.0.0-alpha.16",
    "@angular-eslint/template-parser": "18.0.0-alpha.16",
    "@angular/cli": "18.0.1",
    "@angular/common": "18.0.1",
    "@angular/compiler": "18.0.1",
    "@angular/compiler-cli": "18.0.1",
    "@angular/core": "18.0.1",
    "@angular/forms": "18.0.1",
    "@angular/language-service": "18.0.1",
    "@angular/platform-browser": "18.0.1",
    "@angular/platform-browser-dynamic": "18.0.1",
    "@angular/router": "18.0.1",
    "@commitlint/config-conventional": "19.2.2",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@types/jasmine": "5.1.4",
    "@types/jasminewd2": "2.0.13",
    "@types/node": "20.12.12",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "chartist": "1.3.0",
    "commitlint": "19.3.0",
    "core-js": "3.37.1",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsdoc": "48.2.6",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "husky": "9.0.11",
    "jasmine-core": "5.1.2",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.3",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "lint-staged": "15.2.5",
    "ng-packagr": "18.0.0",
    "prettier": "3.2.5",
    "prettier-plugin-organize-imports": "3.2.4",
    "prettier-plugin-packagejson": "2.5.0",
    "puppeteer": "22.10.0",
    "rxjs": "7.8.1",
    "semantic-release": "23.1.1",
    "ts-node": "10.9.2",
    "tslib": "2.6.2",
    "typescript": "5.4.5",
    "zone.js": "0.14.6"
  },
  "packageManager": "pnpm@9.1.3",
  "volta": {
    "node": "20.14.0",
    "pnpm": "9.1.3"
  }
}
