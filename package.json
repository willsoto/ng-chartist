{
  "name": "ng-chartist",
  "version": "0.0.0",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "build": "ng build ng-chartist",
    "checkstyle": "prettier . --check",
    "e2e": "ng e2e",
    "format": "prettier . --write",
    "lint": "ng lint ng-chartist",
    "ng": "ng",
    "prepare": "husky install",
    "prerelease": "npm run build",
    "release": "semantic-release",
    "start": "ng serve ng-chartist-demo",
    "test": "ng test ng-chartist --watch=false",
    "test:coverage": "ng test ng-chartist --watch=false --code-coverage",
    "typecheck": "npm run build",
    "upgrade-deps": "ncu --upgrade --peer --doctor"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,js,md,json,html}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSameLine": true,
    "bracketSpacing": true,
    "semi": true,
    "singleQuote": false
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "dist/ng-chartist"
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/git"
    ]
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular-eslint/builder": "~13.1.0",
    "@angular-eslint/eslint-plugin": "~13.1.0",
    "@angular-eslint/eslint-plugin-template": "~13.1.0",
    "@angular-eslint/schematics": "~13.1.0",
    "@angular-eslint/template-parser": "~13.1.0",
    "@angular/cli": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/language-service": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@commitlint/config-conventional": "~16.2.1",
    "@semantic-release/changelog": "~6.0.1",
    "@semantic-release/git": "~10.0.1",
    "@types/chartist": "~0.11.1",
    "@types/jasmine": "4.0.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "~17.0.22",
    "@typescript-eslint/eslint-plugin": "~5.16.0",
    "@typescript-eslint/parser": "~5.16.0",
    "chartist": "~0.11.4",
    "chartist-plugin-tooltips-updated": "~0.1.4",
    "commitlint": "~16.2.3",
    "core-js": "~3.21.1",
    "eslint": "~8.11.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-import": "~2.25.4",
    "eslint-plugin-jsdoc": "~38.0.6",
    "eslint-plugin-prefer-arrow": "~1.2.3",
    "husky": "~7.0.4",
    "jasmine-core": "~4.0.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.17",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "~12.3.7",
    "ng-packagr": "~13.3.0",
    "npm-check-updates": "~12.5.4",
    "prettier": "~2.6.0",
    "prettier-plugin-organize-imports": "~2.3.4",
    "prettier-plugin-packagejson": "~2.2.16",
    "puppeteer": "~13.5.1",
    "rxjs": "~7.5.5",
    "semantic-release": "~19.0.2",
    "ts-node": "~10.7.0",
    "tslib": "~2.3.1",
    "typescript": "~4.6.2",
    "zone.js": "~0.11.5"
  },
  "volta": {
    "node": "16.14.0",
    "npm": "8.5.2"
  }
}
