{
  "name": "ng-chartist",
  "version": "0.0.0",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "build": "ng build ng-chartist",
    "checkstyle": "prettier . --check",
    "e2e": "ng e2e",
    "format": "prettier . --write",
    "lint": "ng lint ng-chartist",
    "ng": "ng",
    "prepare": "husky install",
    "release": "semantic-release",
    "test": "ng test ng-chartist --watch=false",
    "test:coverage": "ng test ng-chartist --watch=false --code-coverage",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,js,md,json,html}": [
      "prettier --write"
    ]
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "dist/ng-chartist"
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/git"
    ]
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.10",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "14.2.10",
    "@angular/common": "14.2.11",
    "@angular/compiler": "14.2.11",
    "@angular/compiler-cli": "14.2.11",
    "@angular/core": "14.2.11",
    "@angular/forms": "14.2.11",
    "@angular/language-service": "14.2.11",
    "@angular/platform-browser": "14.2.11",
    "@angular/platform-browser-dynamic": "14.2.11",
    "@angular/router": "14.2.11",
    "@commitlint/config-conventional": "17.2.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@types/jasmine": "4.3.0",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "chartist": "1.3.0",
    "commitlint": "17.2.0",
    "core-js": "3.26.1",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.6.2",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "husky": "8.0.1",
    "jasmine-core": "4.5.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "lint-staged": "13.0.3",
    "ng-packagr": "14.2.2",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "3.1.1",
    "prettier-plugin-packagejson": "2.3.0",
    "puppeteer": "19.2.2",
    "rxjs": "7.5.7",
    "semantic-release": "19.0.5",
    "ts-node": "10.9.1",
    "tslib": "2.4.1",
    "typescript": "4.8.4",
    "zone.js": "0.12.0"
  },
  "packageManager": "pnpm@7.14.2",
  "volta": {
    "node": "18.12.0",
    "npm": "8.19.3"
  }
}
